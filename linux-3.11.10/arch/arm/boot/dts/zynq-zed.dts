/dts-v1/;
/include/ "zynq-7000.dtsi"

/ {
	model = "Zynq Zed Development Board";
	compatible = "xlnx,zynq-7000";

	memory {
		device_type = "memory";
		reg = <0 0x20000000>;
	};

	chosen {
		bootargs = "console=ttyPS0,115200 earlyprintk";
	};

};

&uart1 {
	status = "okay";
};
