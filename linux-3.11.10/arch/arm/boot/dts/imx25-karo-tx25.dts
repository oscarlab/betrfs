/dts-v1/;
#include "imx25.dtsi"

/ {
	model = "Ka-Ro TX25";
	compatible = "karo,imx25-tx25", "fsl,imx25";

	memory {
		reg = <0x80000000 0x02000000 0x90000000 0x02000000>;
	};
};

&uart1 {
	status = "okay";
};

&fec {
	phy-mode = "rmii";
	status = "okay";
};

&nfc {
	nand-on-flash-bbt;
	status = "okay";
};
