config SOLO6X10
	tristate "Softlogic 6x10 MPEG codec cards"
	depends on PCI && VIDEO_DEV && SND && I2C
	select FONT_SUPPORT
	select FONT_8x16
	select VIDEOBUF2_DMA_SG
	select VIDEOBUF2_DMA_CONTIG
	select SND_PCM
	select FONT_8x16
	---help---
	  This driver supports the Softlogic based MPEG-4 and h.264 codec
	  cards.
